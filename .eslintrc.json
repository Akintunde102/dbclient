{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "args": "none" }],
    "new-cap": ["error", { "properties": false }]
  },
  "globals": {
    "Server": true,
    "conf": true,
    "connection": true
}
}
